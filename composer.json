{
    "name": "frknakk/laravel-internetmarke",
    "description": "A Internetmarke and ProdWS Wrapper for Laravel",
    "type": "project",
    "license": "MIT",
    "keywords": ["laravel", "internetmarke", "prodws", "eporto", "portokasse"],
    "authors": [
        {
            "name": "Furkan Akkoc",
            "email": "furkan.akkoc@fatech.dev"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Frknakk\\Internetmarke\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Frknakk\\Internetmarke\\ServiceProvider"
            ],
            "aliases": {
                "ProdWS": "Frknakk\\Internetmarke\\ProdWS\\Facade",
                "Internetmarke": "Frknakk\\Internetmarke\\Facade"
            }
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-soap": "*",
        "ext-xml": "*",
        "illuminate/support": "^6|^7|^8|^9",
        "robrichards/wse-php": "^2.0"
    }
}
